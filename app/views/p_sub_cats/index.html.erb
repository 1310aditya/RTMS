<p id="notice"><%= notice %></p>

<h1>Listing P Sub Cats</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Desc</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @p_sub_cats.each do |p_sub_cat| %>
      <tr>
        <td><%= p_sub_cat.title %></td>
        <td><%= p_sub_cat.desc %></td>
        <td><%= link_to 'Show', p_sub_cat %></td>
        <td><%= link_to 'Edit', edit_p_sub_cat_path(p_sub_cat) %></td>
        <td><%= link_to 'Destroy', p_sub_cat, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New P sub cat', new_p_sub_cat_path %>
